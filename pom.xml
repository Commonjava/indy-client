  <parent>
    <groupId>org.commonjava</groupId>
    <artifactId>commonjava</artifactId>
    <version>21</version>
  </parent>

  <groupId>org.commonjava.indy</groupId>
  <artifactId>indy-clients-parent</artifactId>
